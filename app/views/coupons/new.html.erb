<h1>Coupon Form</h1>
<%= form_tag new_coupon_path do %>
    
    <%= label_tag "Coupon code:" %>
    <%= text_field_tag :"coupon[coupon_code]" %><br /><br />
    
    <%= label_tag "Store name:" %>
    <%= text_field_tag :"coupon[store]" %><br /><br />
    
    <%= submit_tag "Submit Coupon" %>

<% end %>

<%= params.inspect %>
